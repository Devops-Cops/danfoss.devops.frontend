pipeline {
  agent any
 
  tools {nodejs "node"}
 
  stages {
    stage('Cloning Git') {
      steps {
        git 'https://github.com/Devops-Cops/danfoss.devops.frontend.git'
      }
    }
    stage('Checkout') {
        steps {
            checkout([
            $class: 'GitSCM',
            branches: scm.branches,
            extensions: scm.extensions + [[$class: 'LocalBranch'], [$class: 'WipeWorkspace']],
            userRemoteConfigs: [],
            doGenerateSubmoduleConfigurations: false
            ])
        }
    }
  }
}
